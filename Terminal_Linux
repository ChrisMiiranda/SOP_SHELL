questão 1: 
	echo "Christopher Ramos Miranda"

questão 2:
	vi cliente_1.txt
	echo "Christopher Ramos Miranda">cliente_1.txt
	less cliente_1.txt

questão 3:
	echo "Joinville">>cliente_1.txt

questão 4:
	mkdir cliente
	ls

questão 5:
	mv cliente_1.txt cliente
	cd cliente
	ls

questão 6: 
	cp cliente_1.txt cliente_1.txt.bkp
	ls

questão 7:
	rm cliente_1.txt
	ls

questão 8:
	vi cliente.script
		echo "Christopher Ramos Miranda"
		vi cliente_1.txt
		echo "Christopher Ramos Miranda">cliente_1.txt
		less cliente_1.txt
		echo "Joinville">>cliente_1.txt
		mkdir cliente
		ls
		mv cliente_1.txt cliente
		cd cliente
		ls
		cp cliente_1.txt cliente_1.txt.bkp
		rm cliente_1.txt

questão 9:	
	chmod +x cliente.script
	./cliente.script


Questão 10:

	O comando cal imprime um calendário. 


